    "bootstrap-icons": "^1.11.3",
    "cookies-next": "^2.1.1",
    "core-js": "^3.26.0",
    "next": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "primeicons": "^6.0.1",
    "primereact": "^9.6.0",
    "provenance-ui": "github:sennetconsortium/provenance-ui#v2.6.5-rc",
    "react-data-table-component": "^7.6.2",
    "react-helmet-async": "^2.0.3",
    "react-idle-timer": "^5.7.2",
    "react-joyride": "^2.7.0",
    "react-toastify": "^9.1.1",
